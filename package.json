{
  "name": "monorepo_deployment",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for multiple packages",
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@4.10.3",
  "scripts": {
    "start": "yarn workspace @monorepo/example-package start",
    "start:example": "yarn workspace @monorepo/example-package start",
    "start:core": "yarn workspace @monorepo/core-package start",
    "start:all": "yarn workspaces foreach -piv run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn workspaces foreach -piv run build",
    "test:all": "yarn workspaces foreach -piv run test",
    "clean": "yarn workspaces foreach -piv run clean"
  },
  "author": "",
  "license": "ISC"
}
